diff --git a/addons/script.osmc.android/addon.py b/addons/script.osmc.android/addon.py
new file mode 100644
index 0000000..f2242f3
--- /dev/null
+++ b/addons/script.osmc.android/addon.py
@@ -0,0 +1,9 @@
+import xbmcaddon
+import xbmcgui
+import os
+
+try:
+    os.path.stat.S_ISBLK(os.stat("/dev/mmcblk0p1").st_mode)
+    os.system("/usr/bin/sudo /sbin/reboot androidv2")
+except:
+    xbmcgui.Dialog().ok("Card not detected", "Please insert an SD card with Android first. See osmc.tv/wiki for more information")
diff --git a/addons/script.osmc.android/addon.xml b/addons/script.osmc.android/addon.xml
new file mode 100644
index 0000000..1af62e6
--- /dev/null
+++ b/addons/script.osmc.android/addon.xml
@@ -0,0 +1,16 @@
+<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
+<addon id="script.osmc.androidboot" name="Boot to Android" version="1.0.0" provider-name="osmc">
+	<requires>
+		<import addon="xbmc.python" version="2.14.0"/>
+	</requires>
+	<extension point="xbmc.python.script" library="addon.py">
+		<provides>executable</provides>
+	</extension>
+	<extension point="xbmc.addon.metadata">
+		<platform>all</platform>
+		<summary lang="en">Boot to Android</summary>
+		<description lang="en">Reboots Vero 2 to Android</description>
+		<license>GNU General Public License, v2</license>
+		<language></language>
+	</extension>
+</addon>
